{
  "name": "ssh-manager",
  "version": "0.1.0",
  "description": "A cross-platform GUI application for managing SSH configurations through organized folders and visual forms",
  "main": "src/main.js",
  "bin": {
    "ssh-manager": "./bin/ssh-manager.js"
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "test": "jest",
    "lint": "eslint src/",
    "create-shortcut": "node bin/ssh-manager.js create-shortcut"
  },
  "keywords": [
    "ssh",
    "configuration",
    "management",
    "network",
    "electron",
    "desktop"
  ],
  "author": "Jeremy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/isriam/ssh_manager.git"
  },
  "bugs": {
    "url": "https://github.com/isriam/ssh_manager/issues"
  },
  "homepage": "https://github.com/isriam/ssh_manager#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "electron": "^28.0.0",
    "ssh-config": "^4.1.6",
    "fs-extra": "^11.1.1",
    "commander": "^11.1.0",
    "node-ssh": "^13.1.0"
  },
  "devDependencies": {
    "electron-builder": "^24.6.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0"
  },
  "build": {
    "appId": "com.ssh-manager.app",
    "productName": "SSH Manager",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*",
      "bin/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}