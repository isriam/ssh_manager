{
  "name": "ssh-manager",
  "version": "0.1.2",
  "description": "A cross-platform GUI application for managing SSH configurations through organized folders and visual forms",
  "main": "src/main.js",
  "bin": {
    "ssh-manager": "./bin/ssh-manager.js"
  },
  "scripts": {
    "start": "if [ ! -d node_modules ]; then echo 'Installing dependencies...' && npm install; fi && electron .",
    "dev": "electron . --dev",
    "create-shortcut": "node bin/ssh-manager.js create-shortcut",
    "postinstall": "node scripts/postinstall.js",
    "preuninstall": "node scripts/preuninstall.js"
  },
  "keywords": [
    "ssh",
    "configuration",
    "management",
    "network",
    "electron",
    "desktop"
  ],
  "author": "Jeremy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/isriam/ssh_manager.git"
  },
  "bugs": {
    "url": "https://github.com/isriam/ssh_manager/issues"
  },
  "homepage": "https://github.com/isriam/ssh_manager#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "archiver": "^6.0.2",
    "commander": "^11.1.0",
    "electron": "^28.0.0",
    "fs-extra": "^11.1.1",
    "node-ssh": "^13.1.0",
    "ssh-config": "^4.1.6"
  }
}
